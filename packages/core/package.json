{
  "name": "@uni-helper/vite-plugin-uni-components",
  "type": "module",
  "version": "0.2.4",
  "packageManager": "pnpm@10.27.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/vite-plugin-uni-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/vite-plugin-uni-components.git"
  },
  "bugs": "https://github.com/uni-helper/vite-plugin-uni-components/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    "./resolvers": {
      "import": {
        "types": "./dist/resolvers.d.mts",
        "default": "./dist/resolvers.mjs"
      },
      "require": {
        "types": "./dist/resolvers.d.cts",
        "default": "./dist/resolvers.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "chokidar": "^4.0.3",
    "debug": "^4.4.3",
    "local-pkg": "^1.1.2",
    "magic-string": "^0.30.21",
    "minimatch": "^10.1.1",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@antfu/utils": "^9.3.0",
    "@types/debug": "^4.1.12",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^25.0.10",
    "estree-walker": "^3.0.3",
    "pathe": "^2.0.3",
    "rollup": "^4.54.0",
    "vite": "5.2.8"
  }
}
